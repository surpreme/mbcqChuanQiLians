<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.alllocalagent.localagent.LocalAgentActivity">

    <com.mbcq.baselibrary.view.CnToolbar
        android:id="@+id/local_agent_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/base_blue"
        app:backButtonIsShow="true"
        app:centerTextViewText="外转记录"
        app:layout_constraintTop_toTopOf="parent"
        app:rightButtonIcon="@drawable/ic_search_icon" />

    <com.flyco.tablayout.CommonTabLayout
        android:id="@+id/local_agent_tabLayout"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/local_agent_toolbar"
        app:tl_iconVisible="false"
        app:tl_indicator_bounce_enable="false"
        app:tl_indicator_color="@color/base_blue"
        app:tl_indicator_gravity="BOTTOM"
        app:tl_indicator_height="3dp"
        app:tl_indicator_width="115dp"
        app:tl_textSelectColor="@color/base_blue"
        app:tl_textUnselectColor="@color/black"
        app:tl_textsize="15sp"
        app:tl_underline_color="@color/white"
        app:tl_underline_gravity="BOTTOM"
        app:tl_underline_height="3dp" />

    <FrameLayout
        android:id="@+id/local_agent_fragment_fl"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/local_agent_tabLayout" />

</androidx.constraintlayout.widget.ConstraintLayout>